{
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    "MimeTypes": []
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "*"
    ],
    "ForwardedHeaders": "XForwardedHost,XForwardedProto"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 102400,
      "MaxRequestHeaderCount": 40
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "MassTransit": "Debug",
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Theme": "Code"
      },
      {
        "Name": "Debug"
      }
    ]
  },
  "AllowedHosts": "*"
}