{
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    "MimeTypes": [
      "application/vnd.restful+json",
      "application/problem+json"
    ]
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "*"
    ],
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 102400,
      "MaxRequestHeaderCount": 40
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "MassTransit": "Information",
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Theme": "Code"
      },
      {
        "Name": "Debug"
      }
    ]
  },
  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Information",
  //    "Elastic.Apm": "Debug"
  //  }
  //},
  //"ElasticApm": {
  //  "LogLevel": "Trace",
  //  "ServerUrls": "https://apm:443",
  //  "ServiceName": "TenantWorker",
  //  "SecretToken": "token",
  //  "TransactionSampleRate": 1.0
  //},
  "AllowedHosts": "*",
  "AWS": {
    "Region": "eu-central-1",
    "Profile": "default"
  },
  "HostOptions": {
    "ShutdownTimeout": "00:00:15"
  },
  "General": {
    "ClientRequestIdHeader": "x-ya-client-request-id",
    "CorrelationIdHeader": "x-ya-request-id",
    "MaxLogFieldLength": 27716,
    "DefaultPaginationPageSize": 5,
    "IdempotencyFilterEnabled": true
  }
}
